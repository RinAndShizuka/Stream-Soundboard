<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="window" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="561.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rin.soundboard.PrimaryController">
    <children>
        <Label prefHeight="20.0" prefWidth="160.0" text="Select VAC/Voicemeeter input" textAlignment="CENTER" translateY="-50.0" />
        <ComboBox id="comboBox" fx:id="comboBox" prefHeight="25.0" prefWidth="272.0" translateY="-50.0" />
        <Label prefHeight="20.0" prefWidth="135.0" text="Type Link or Search Term" textAlignment="CENTER" translateY="-50.0" />
        <TextField fx:id="textbox" maxWidth="-Infinity" minWidth="-Infinity" onAction="#onAction" onKeyPressed="#keyPressed" prefHeight="26.0" prefWidth="267.0" translateY="-50.0">
            <font>
                <Font name="Arial" size="12.0" />
            </font></TextField>
      <Button mnemonicParsing="false" onMouseClicked="#mouseClicked" text="Skip" />
      <Slider fx:id="slider" onMouseDragged="#dragDetected" onTouchMoved="#dragDetected" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" value="100.0" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
